{
  "name": "burger-queen-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "pretest": "eslint .",
    "start": "node index.js",
    "test:e2e": "jest --verbose --testPathPattern e2e --globalSetup ./e2e/globalSetup --globalTeardown ./e2e/globalTeardown --runInBand",
    "test:unit": "jest --verbose --testPathIgnorePatterns mock-express e2e setup teardown --globalSetup ./controller/__tests__/setup --globalTeardown ./controller/__tests__/teardown --runInBand --coverage",
    "test": "npm run test:e2e && npm run test:unit",
    "lint": "eslint .",
    "docs:generate": "jsdoc -c jsdoc.conf --readme README-docs.md",
    "nodemon": "nodemon index.js",
    "docs:deploy": "rm -rf docs && npm run docs:generate && gh-pages -d docs"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "jsonwebtoken": "8.5.1",
    "mongoose": "^5.9.21",
    "mongoose-paginate-v2": "^1.3.9",
    "mongoose-unique-validator": "^2.0.3",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.1",
    "docdash": "1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gh-pages": "2.1.1",
    "jest": "^26.1.0",
    "jsdoc": "3.6.3",
    "jsdoc-http-plugin": "0.3.1",
    "node-fetch": "2.6.0",
    "tree-kill": "^1.2.2"
  }
}
